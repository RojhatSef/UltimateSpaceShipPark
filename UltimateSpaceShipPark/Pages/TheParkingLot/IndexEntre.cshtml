@page
@model UltimateSpaceShipPark.Pages.TheParkingLot.IndexEntreModel
@{
    ViewData["ShowButtons"] = true;

}

<!-- Shows the search bar-->


<h4>@Model.FormResult</h4>

<h1>Parking Spots</h1>


<div class="row">
    <div class="col-12">
        <div class="card">
        <div class="card-body">
                             <div class="card-deck">
            @foreach (var ParkingLotModel in Model.ParkingSpot)
            {
                <partial name="/Pages/Shared/_DisplayParkingLots.cshtml" model="ParkingLotModel" view-data="ViewData" />
  
            }
                                      </div>
                    </div>
        </div>
    </div>
</div>